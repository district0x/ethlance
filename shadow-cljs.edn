 :nrepl {:middleware [cider.piggieback/wrap-cljs-repl
                      refactor-nrepl.middleware/wrap-refactor]
         :port 54872}

 :http {:port 3449
        :host "0.0.0.0"}

 :builds {:dev-ui
          {:target :browser
           :modules {:main {:init-fn ethlance.ui.core/init}}
           :main ethlance.ui.core
           :infer-externs true
           :js-options {:js-package-dirs ["./ui/node_modules"]}
           :npm-deps false
           :output-dir "resources/public/js/"
           :optimizations :none
           :source-map true
           :source-map-timestamp true
           :devtools {:ignore-warnings true}
           :preloads [devtools.preload
                      re-frisk.preload]
           :closure-defines {"re_frame.trace.trace_enabled_QMARK_" true
                             goog.DEBUG true}}

          :dev-server
          {:target :node-script
           :main ethlance.server.core/-main
           :js-options {:js-package-dirs ["./server/node_modules"]}
           :output-dir "server/"
           :output-to "server/ethlance_server.js"
           :optimizations :none
           :source-map true
           :source-map-timestamp true
           :closure-defines {goog.DEBUG true}}

          :server-tests
          {:target :node-test
           :modules {:main {:init-fn tests.runner/start-and-run-tests}}
           :js-options {:js-package-dirs ["./server/node_modules"]}
           :output-dir "server/tests"
           :output-to "server/tests/server-tests.js"
           :ns-regexp "tests."
           :optimizations :none
           ;; needs to be set to false to report correct status code in CI
           :autorun false
           :closure-defines {goog.DEBUG true}}}}
